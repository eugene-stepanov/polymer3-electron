{
  "entrypoint": "index.html",
  "fragments": [],
  "sources": [
    "src/**.js",
    "manifest/**",
    "package.json",
    ".npmrc",
    "index.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "name": "es6-bundled",
      "js": {
        "compile": "es2015",
        "minify": false,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": false
      },
      "html": {
        "minify": false
      },
      "bundle": true,
      "addServiceWorker": true
    }
  ],
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "npm": true,
  "componentDir": "node_modules/",
  "moduleResolution": "node"
}